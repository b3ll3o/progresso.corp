{
  "name": "@monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:affected": "nx affected --target=build",
    "build:api": "nx build api",
    "build:frontend": "nx build frontend",
    "dev": "nx run-many --target=serve --all --parallel",
    "dev:api": "nx serve api",
    "dev:frontend": "nx dev frontend",
    "start": "nx run-many --target=serve --all --parallel --configuration=production",
    "start:api": "nx serve api --configuration=production",
    "start:frontend": "nx start frontend",
    "test": "nx run-many --target=test --all",
    "test:affected": "nx affected --target=test",
    "test:api": "nx test api",
    "test:frontend": "nx test frontend",
    "test:e2e": "nx run-many --target=e2e --all",
    "lint": "nx run-many --target=lint --all",
    "lint:affected": "nx affected --target=lint",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prisma:generate": "nx prisma-generate api",
    "prisma:migrate": "nx prisma-migrate api",
    "prisma:push": "nx prisma-push api",
    "prisma:studio": "npx prisma studio --schema=apps/api/prisma/schema.prisma",
    "prisma:seed": "npx prisma db seed --schema=apps/api/prisma/schema.prisma",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "clean": "nx reset && rm -rf dist node_modules/.cache",
    "clean:full": "npm run clean && rm -rf node_modules && npm ci",
    "graph": "nx graph",
    "graph:affected": "nx affected:graph",
    "migrate": "nx migrate latest",
    "migrate:run": "npx nx migrate --run-migrations",
    "workspace:lint": "nx workspace-lint",
    "affected": "nx affected",
    "affected:apps": "nx print-affected --type=app",
    "affected:libs": "nx print-affected --type=lib",
    "update:deps": "npx npm-check-updates -i",
    "setup": "npm ci && npm run prisma:generate && npm run build",
    "prepare": "husky install || true"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
